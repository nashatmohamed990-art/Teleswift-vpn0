### APP ###
APP_PORT=3000
METRICS_PORT=3001

### API ###
API_INSTANCES=1

### DATABASE ###
# Railway سيعطيك DATABASE_URL تلقائياً - ضعها هنا
DATABASE_URL=postgresql://postgres:CHANGE_DB_PASSWORD@remnawave-db:5432/postgres

### REDIS ###
REDIS_HOST=remnawave-redis
REDIS_PORT=6379

### JWT ###
### ⚠️ غيّر هذه القيم - استخدم: openssl rand -hex 64 ###
JWT_AUTH_SECRET=CHANGE_ME_USE_openssl_rand_hex_64
JWT_API_TOKENS_SECRET=CHANGE_ME_USE_openssl_rand_hex_64

### TELEGRAM NOTIFICATIONS ###
IS_TELEGRAM_NOTIFICATIONS_ENABLED=false
TELEGRAM_BOT_TOKEN=CHANGE_ME
TELEGRAM_NOTIFY_USERS_CHAT_ID=CHANGE_ME
TELEGRAM_NOTIFY_NODES_CHAT_ID=CHANGE_ME
TELEGRAM_NOTIFY_CRM_CHAT_ID=CHANGE_ME

### FRONT_END ###
FRONT_END_DOMAIN=*

### SUBSCRIPTION PUBLIC DOMAIN ###
### ⚠️ ضع دومين البانيل هنا بدون https:// ###
### مثال: panel.yourdomain.com/api/sub ###
SUB_PUBLIC_DOMAIN=panel.CHANGE_ME.com/api/sub

### SWAGGER ###
SWAGGER_PATH=/docs
SCALAR_PATH=/scalar
IS_DOCS_ENABLED=false

### PROMETHEUS ###
METRICS_USER=admin
### ⚠️ غيّر هذه القيمة ###
METRICS_PASS=CHANGE_ME_USE_openssl_rand_hex_32

### WEBHOOK - مهم جداً للبوت ###
WEBHOOK_ENABLED=true
### ⚠️ ضع رابط البوت هنا - سيكون من Railway ###
### مثال: https://teleswiftvpn-bot.railway.app/api/v1/remnawave ###
WEBHOOK_URL=https://YOUR_BOT_DOMAIN.railway.app/api/v1/remnawave
### ⚠️ هذه القيمة يجب أن تطابق REMNAWAVE_WEBHOOK_SECRET في ملف البوت ###
WEBHOOK_SECRET_HEADER=CHANGE_ME_EXACTLY_64_CHARS_a_z_A_Z_0_9_XXXX

### Database ###
POSTGRES_USER=postgres
POSTGRES_PASSWORD=CHANGE_DB_PASSWORD
POSTGRES_DB=postgres
